$("#loading").html("Loading...");
var anick = "<%= @username %>";
$.ajax({
  cache: false,
  type: "GET",
  dataType: "json",
  url: "http://localhost:8001/join",  
  data: { nick: anick },
  error: function (xhr, textStatus, errorThrown) {
    alert("error connecting to server ");
    $("#loading").html("");    
  }, 
  success: function(){
    alert("Connected");
  }
});